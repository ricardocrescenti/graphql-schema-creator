  "bugs": {
    "url": "https://github.com/ricardocrescenti/graphql-schema-creator/issues"
  },
  "homepage": "https://github.com/ricardocrescenti/graphql-schema-creator#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "apollo-server-express": "^2.9.12",
    "graphql-type-json": "^0.3.1",
    "jest": "^24.9.0",
    "knex": "^2.4.0",
    "knex-json-where": "^1.2.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.3"
